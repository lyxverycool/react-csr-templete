{
  "name": "csr-templete",
  "version": "0.0.9",
  "description": "csr模板",
  "scripts": {
    "start": "npm run start:dev",
    "build": "npm run build:prod",
    "start:dev": "cross-env NODE_ENV=development DEV_SERVER=true webpack-dev-server --config webpack/webpack.config.js --open",
    "start:prod": "cross-env NODE_ENV=production DEV_SERVER=true webpack-dev-server --config webpack/webpack.config.js --open",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js",
    "lint": "eslint --ext .js --ext .jsx src"
  },
  "author": "lyxcool",
  "dependencies": {
    "@loadable/component": "^5.10.2",
    "antd": "4.1.4",
    "axios": "^0.19.0",
    "lyxcool-test": "^1.0.1",
    "lyxcool-webpack": "2.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "^4.12.21",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0"
  },
  "devDependencies": {
    "typescript": "4.0.3",
    "@types/react": "16.9.8",
    "@types/react-dom": "16.9.8"
  }
}
